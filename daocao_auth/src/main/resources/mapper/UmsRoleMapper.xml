<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daocao.common.mapper.UmsRoleMapper">

    <select id="selectUserById" resultType="long">
        select role_id
        from ums_sys_user_role
        where user_id=#{userId}


    </select>
</mapper>